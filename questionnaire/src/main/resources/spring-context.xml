<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="csvParser" class="ru.otus.dao.CsvParser"/>
    <bean id="questionnaireService" class="ru.otus.service.DefaultQuestionnaireService">
        <property name="fileName" value="questions.csv"/>
        <constructor-arg name="parser" ref="csvParser"/>
    </bean>

</beans>
